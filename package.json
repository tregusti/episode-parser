{
  "name": "episode-parser",
  "version": "1.5.0",
  "description": "Parse file names of tv show episodes.",
  "main": "dist/parser.js",
  "scripts": {
    "release": "standard-version",
    "style": "./node_modules/.bin/eslint .",
    "test": "npm run style && npm run unittest",
    "unittest": "npm run build && ./node_modules/.bin/mocha --reporter=dot",
    "build": "tsc",
    "watch": "npm run build -- -w",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tregusti/episode-parser.git"
  },
  "keywords": [
    "episode",
    "filename",
    "parse",
    "parser",
    "show",
    "tv"
  ],
  "author": "Glenn Jorde <glenn@tregusti.com>",
  "contributors": [
    "Jeffrey Burt (https://github.com/Jephuff)"
  ],
  "license": {
    "type": "MIT",
    "url": "http://tregusti.mit-license.org/"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.4.0",
    "mocha": "^8.0.1",
    "standard-version": "^8.0.2",
    "typescript": "^3.9.6"
  }
}
